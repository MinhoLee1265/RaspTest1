          <h1>Ridar 센서 로그2</h1>
          
          <h2><div id="temp_box" class="temp_log">
          </div></h2>

        
       
    <script>
      var socket = io();

      socket.on('receive message', function(msg){
        msg = msg.replace(/\r/g, '');
        if(msg!="" || msg!=null)
        {
          //console.log(msg + "111");
          //$("#temp_box").append(`${msg} </br>`);  
          $("#temp_box").text(`${msg}`);  
        }
      });

      
      function led_on(){
        console.log('on')
        socket.emit('send message',"1");      
      }
      
      function led_off(){
        console.log('off')
        socket.emit('send message',"0");      
      }


    </script>
  </body>
